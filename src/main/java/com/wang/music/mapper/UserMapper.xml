<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wang.music.mapper.UserMapper">
    <!--根据ID查询用户-->
    <select id="queryUserByUserId" resultType="user" parameterType="int">
        select * from  tb_user
        where userId=#{userId}
    </select>
</mapper>